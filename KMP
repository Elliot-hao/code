//UVA210 并行程序模拟 Concurrency Simulator
#include <bits/stdc++.h>
using namespace std;
int slen;
int tlen;
int ne[1000000+5];


void get_next(string t)
{
    int j = 0, k = -1;
    ne[0] = -1;
    while (j < tlen)
    {
        if(k == -1 || t[j] == t[k])
            ne[++j] = ++k;
        else
            k = ne[k];
    }

}
void KMP(string s, string t)
{
    int i = 0, j = 0;
    slen = s.length();
    tlen = t.length();
    get_next(t);
    while (i < slen)
    {
        if( j == -1 || s[i] == t[j])
         {
             i++;
             j++;
         }
        else
        {
            j = ne[j];
        }
        if(j == tlen)
        {
            cout << i - tlen +1 <<endl;
            j = ne[j];
        }
    }


}

int main(){
    string s,t;
    cin >> s >> t;
    KMP(s,t);
    for (int i = 1; i <= tlen ;++i) {
        cout << ne[i] <<" ";
    }
    return 0;
}
